<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <!-- Plus sign -->
    <item>
        <rotate
            android:fromDegrees="0"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="@color/black"/> <!-- Warna garis plus -->
            </shape>
        </rotate>
    </item>

    <item android:top="0dp">
        <rotate
            android:fromDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="5dp"
                    android:color="@color/black"/> <!-- Warna garis plus -->
            </shape>
        </rotate>
    </item>

</layer-list>
